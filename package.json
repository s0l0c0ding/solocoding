{
  "name": "solo-coding-blog",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "scully": "scully",
    "scully:serve": "scully serve",
    "buildscully": "npm run build -- --prod --stats-json && npm run scully -- --scanRoutes --showGuessError",
    "scanscully": "npm run scully -- --scanRoutes --showGuessError",
    "copy404": "cp dist/soloCoding-blog/index.html dist/soloCoding-blog/404.html",
    "moveToDocs": "mv dist/static/* docs/",
    "buildGithub": "ng build --prod && npm run copy404 && npm run scanscully && npm run moveToDocs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.9",
    "@angular/common": "^10.0.9",
    "@angular/compiler": "^10.0.9",
    "@angular/core": "^10.0.9",
    "@angular/forms": "^10.0.9",
    "@angular/platform-browser": "^10.0.9",
    "@angular/platform-browser-dynamic": "^10.0.9",
    "@angular/router": "^10.0.9",
    "@scullyio/init": "0.0.29",
    "@scullyio/ng-lib": "0.0.27",
    "@scullyio/scully": "1.0.0-beta.3",
    "bootstrap": "^4.5.2",
    "guess-parser": "^0.4.19",
    "jsdom": "^16.4.0",
    "rxjs": "~6.6.2",
    "tslib": "^2.0.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1000.6",
    "@angular/cli": "^10.0.6",
    "@angular/compiler-cli": "^10.0.9",
    "@angular/language-service": "^10.0.9",
    "@types/jasmine": "^3.5.12",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.6.0",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "protractor": "^7.0.0",
    "scully-plugin-disable-angular": "latest",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.3",
    "typescript": "~3.9.7"
  }
}
